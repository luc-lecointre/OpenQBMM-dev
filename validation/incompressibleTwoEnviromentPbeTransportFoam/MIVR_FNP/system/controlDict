FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     incompressibleMixingReactionFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

// endTime         0.00075;
endTime         .1;

deltaT          1.0e-5;

adjustTimeStep  on;

// writeControl    timeStep;
writeControl    adjustableRunTime;

// writeInterval   1;
writeInterval   1.0e-5;

purgeWrite      10;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   12;

runTimeModifiable true;

maxCo           0.9;

maxDeltaT       1e-3;

