FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      populationaBalanceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

populationBalanceModel univariatePopulationBalance;

univariatePopulationBalanceCoeffs
{
    diffusionModel
    {
        diffusionModel  turbulentDiffusion;
        gammaLam        gammaLam [ 0 2 -1 0 0 0 0 ] 1e-6; 
        Sc              0.7;
    }

    aggregation on;

    aggregationKernel
    {
        aggregationKernel FNP;
        
        m0Scaling   9.0e23;
        nUnimers    100;
        cellVol     cellVol [0 0 -3 0 0 0 0] 1e-28;
        siteVol     siteVol [0 0 -3 0 0 0 0]9.16e-28;
        alpha       1.0;
        na          10;
        nb          68;
        nuA         #calc "1.0/3.0";
        nuB         #calc "3.0/5.0";
        etaS    etaS    [1 -1 -1 0 0 0 0] 0.0021; 
        Xip     Xip     [0 0 0 0 0 0 0] 0.5;
        Xi0     Xi0     [0 0 0 0 0 0 0] 0.8;
        T           293.15;
    }

    breakup off;

    breakupKernel
    {
        breakupKernel constant;
        Cb        Cb        [ 0 0 -1 0 0 0 0 ] 0.02;
    }

    daughterDistribution
    {
        daughterDistribution symmetricFragmentation;
    }

    growth off;

    growthModel
    {
        growthModel constant;

        Cg        Cg        [0 3 -1 0 0 0 0 ] 1.0;
    }

    nucleation off;
    
    nucleationModel
    {
        nucleationModel  none;
    }
    
    interfaceModel off;
    
    ode on;
    
    odeCoeffs
    {
        ATol    1e-8;
        RTol    1e-6;
        fac     0.8;
        facMin  .01;
        facMax  2.0;
    }
    odeSourceTerms
    {
        aggregation on;
        breakup     off;
        growth      off;
        nucleation  off;
    }
    
}





