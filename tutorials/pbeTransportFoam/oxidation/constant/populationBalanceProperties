FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      populationaBalanceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

populationBalanceModel univariatePopulationBalance;

univariatePopulationBalanceCoeffs
{
    aggregation off;
    
    aggregationKernel
    {
        aggregationKernel Fuchs;
        
        Ca        Ca        [ 0 0 -1 0 0 0 0 ] 1.0;
        
        
        df                  1.8;
        nCarbonSoot         64;
    }
    
    breakup off;
    
    breakupKernel
    {
        breakupKernel LuoSvendsen;
        
        Cb        Cb        [ 0 0 0 0 0 0 0 ] 1.0e-3;
        epsilonExp          0.75;
        nuExp              -1.25;
        sizeExp             1.0;
    }
    
    daughterDistribution
    {
        daughterDistribution symmetricFragmentation;
    }
    
    diffusionModel
    {
        diffusionModel  none;
        gammaLam        gammaLam    [ 0 2 -1 0 0 0 0 ] 0.0001;
        Sc              0.7;
    }
    
    growth off;
    
    growthModel
    {
        growthModel constant;
        
        Cg        Cg        [0 3 -1 0 0 0 0 ] 1.0;
    }
    
    convection on;
    
    convectionModel
    {
        convectionModel growthReduction;
    }
    
    nucleation off;
    
    nucleationModel
    {
        nucleationModel noNucleation;
    }
}


