FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0.1";
    object      A4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    wall
    {
        type            zeroGradient;
    }
    fuel
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 
0
;
        value           nonuniform 
0
;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 0;
    }
}


