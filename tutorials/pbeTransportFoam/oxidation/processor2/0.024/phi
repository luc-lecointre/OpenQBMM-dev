FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0.024";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -1 0 0 0 0];

internalField   uniform 0.000241844;

boundaryField
{
    wall
    {
        type            calculated;
        value           uniform 0;
    }
    fuel
    {
        type            calculated;
        value           nonuniform 0();
    }
    outlet
    {
        type            calculated;
        value           nonuniform 0();
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform -0.000241844;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 0.000241844;
    }
}


