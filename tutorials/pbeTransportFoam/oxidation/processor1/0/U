FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 4.42594);

boundaryField
{
    wall
    {
        type            zeroGradient;
    }
    fuel
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        value           nonuniform 
0
;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 4.42594);
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform (0 0 4.42594);
    }
}


