FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

H 0.008;
w 0.0176777;
nw -0.0176777;

H0 #calc "$H/100";
H1 #calc "$H0*3";
H2 #calc "$H0*6";
H3 #calc "$H0*9";
H4 #calc "$H0*14";
H5 #calc "$H0*25";
H6 #calc "$H0*36";
H7 #calc "$H0*47";
H8 #calc "$H0*58";
H9 #calc "$H0*69";

defaultFieldValues
(
    volScalarFieldValue T 353.0
);

regions
(
    boxToCell
    {
        box ($nw $nw 0) ($w $w $H1);
        fieldValues
        (
            volScalarFieldValue T 400.0
        );
    }
    
    boxToCell
    {
        box ($nw $nw $H1) ($w $w $H2);
        fieldValues
        (
            volScalarFieldValue T 873.0
        );
    }
    boxToCell
    {
        box ($nw $nw $H2) ($w $w $H3);
        fieldValues
        (
            volScalarFieldValue T 1573.0
        );
    }
    
    boxToCell
    {
        box ($nw $nw $H3) ($w $w $H4);
        fieldValues
        (
            volScalarFieldValue T 2003.0
        );
    }
    boxToCell
    {
        box ($nw $nw $H4) ($w $w $H5);
        fieldValues
        (
            volScalarFieldValue T 1673.0
        );
    }
    
    boxToCell
    {
        box ($nw $nw $H5) ($w $w $H6);
        fieldValues
        (
            volScalarFieldValue T 1473.0
        );
    }
    boxToCell
    {
        box ($nw $nw $H6) ($w $w $H7);
        fieldValues
        (
            volScalarFieldValue T 1250.0
        );
    }
    
    boxToCell
    {
        box ($nw $nw $H7) ($w $w $H8);
        fieldValues
        (
            volScalarFieldValue T 1000.0
        );
    }
    boxToCell
    {
        box ($nw $nw $H8) ($w $w $H9);
        fieldValues
        (
            volScalarFieldValue T 700.0
        );
    }
    
    boxToCell
    {
        box ($nw $nw $H9) ($w $w $H);
        fieldValues
        (
            volScalarFieldValue T 353.0
        );
    }
);


