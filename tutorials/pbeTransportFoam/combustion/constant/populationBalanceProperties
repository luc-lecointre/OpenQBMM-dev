FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      populationaBalanceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

populationBalanceModel univariate;

univariateCoeffs
{
    aggregation off;
    
    aggregationKernel
    {
        aggregationKernel Fuchs;
        
        Ca        Ca        [ 0 0 -1 0 0 0 0 ] 1.0;
        
        df                  1.8;
        nCarbonSoot         64;
    }
    
    breakup off;
    
    breakupKernel
    {
        breakupKernel constant;
        
        Cb        Cb        [ 0 0 -1 0 0 0 0 ] 5.0e-2;
    }
    
    daughterDistribution
    {
        daughterDistribution symmetricFragmentation;
    }
    
    diffusionModel
    {
        diffusionModel  none;
        gammaLam        gammaLam    [ 0 2 -1 0 0 0 0 ] 0.0001;
        Sc              0.7;
    }
    
    convection on;
    
    convectionModel
    {
        convectionModel growthReduction;
    }
    
    nucleation on;
    
    nucleationModel
    {
        nucleationModel Miller;
        
        PAH             "A2 A2R5 A3 A4";
        nCarbonPAH      "10 12 14 16";
        gamma           "0.0010 0.0030 0.0085 0.0150 0.0250";
    }
    interfaceModel off;
    ode on;    
    odeCoeffs
    {
        ATol    1.0e-8;
        RTol    1.0e-6;
        fac     0.8;
        facMin  1e-3;
        facMax  2.0;
    }
}

